[{
    /* TWITTER
     * Domain is twitter.com or t.co
     */
    "pattern": "(?:twitter\\.com|t\\.co)",
    "result": {
        "type": "social",
        "name": "Twitter"
    }
}, {
    /* GOOGLE SEARCH
     * Domain contains 'google' and url contains either
     * '/url?' OR '/search?'
     */
    "pattern": "(?:google).*(?:\\/url\\?|\\/search\\?)",
    "result": {
        "type": "search",
        "name": "Google Search"
    }
}]